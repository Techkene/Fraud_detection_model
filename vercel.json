{
  "version": 2,
  "builds": [
    {
      "src": "predict.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "predict.py"
    }
  ],
  "functions": {
    "predict.py": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}